					"RestAPI_LinkedService_toBlob_Bronze_properties_typeProperties_url": {
						"value": "[parameters('RestAPI_LinkedService_toBlob_Bronze_properties_typeProperties_url')]"
					},
					"triggerWhenTempIsAbove32Degree_properties_typeProperties_scope": {
						"value": "[parameters('triggerWhenTempIsAbove32Degree_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "capstonedxcdf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'capstonedxcdf_ArmTemplate_0')]"
			]
		}
	]
}